{
  "extends": "@brickifyio/tsconfig/nextjs.json",
  "compilerOptions": {
    "jsx": "preserve",
    "types": [
      "node",
      "jest",
      "@testing-library/jest-dom",
      "@testing-library/react"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
